import "standard/string.rf";
import "standard/io.rf";

struct just_a_struct {
    int alpha;
    int beta;
}

just_a_struct operator+(just_a_struct a, just_a_struct b) {
    return (just_a_struct) { .alpha = a.alpha + b.alpha, .beta = a.beta + b.beta };
}

int main() {
    just_a_struct a = (just_a_struct) { .alpha = 2, .beta = 3 };
    just_a_struct b = (just_a_struct) { .alpha = 3, .beta = 2 };
    just_a_struct c = a + b;
    printf("Alpha: %d, Beta: %d\n", c.alpha, c.beta);

    std.string s1 = std.str_get("foo");
    std.string s2 = std.str_get("bar");

    std.str_append(&s1, 'd');

    std.str_concat(&s1, &s2);

    std.string s3 = "foo" + "baz";

    printf("%s\n", std.str_get_cstr(s1));
    printf("%s\n", std.str_get_cstr(s3));

    return 0;
}
